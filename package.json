{
    "name": "boids",
    "description": "An implementation of boids",
    "repository": {
        "type": "git",
        "url": "https://github.corm/fildon/fildon.github.io"
    },
    "license": "MIT",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "build": "tsc && browserify dist/src/app.js -o bundle.js",
        "full": "npm run build && npm run test && npm run lint",
        "lint": "tslint -c tslint.json \"@(src|test)\"/**/*.ts",
        "test": "nyc mocha"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "dependencies": {
        "@types/node": "^9.4.5",
        "typescript": "^2.7.1"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "browserify": "^16.1.0",
        "chai": "^4.1.2",
        "mocha": "^5.0.0",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1"
    }
}
