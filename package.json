{
    "name": "boids",
    "description": "An implementation of boids",
    "repository": {
        "type": "git",
        "url": "https://github.corm/fildon/fildon.github.io"
    },
    "license": "MIT",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "build": "tsc && browserify dist/src/app.js -o bundle.js",
        "full": "npm run build && npm run coverage && npm run lint",
        "lint": "tslint -c tslint.json src/**/*.ts",
        "mutation": "stryker run",
        "test": "nyc mocha",
        "coverage": "nyc --check-coverage --lines 85 mocha"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "dependencies": {
        "@types/knockout": "^3.4.51",
        "@types/node": "^9.4.5",
        "knockout": "^3.5.0-beta",
        "typescript": "^2.9.2"
    },
    "devDependencies": {
        "@stryker-mutator/core": "^1.3.1",
        "@stryker-mutator/mocha-framework": "^1.3.1",
        "@stryker-mutator/mocha-runner": "^1.3.1",
        "@stryker-mutator/typescript": "^1.3.1",
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/sinon": "^5.0.2",
        "browserify": "^16.1.0",
        "chai": "^4.1.2",
        "mocha": "^5.0.0",
        "sinon": "^6.1.5",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1"
    }
}
